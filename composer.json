{
    "name": "watson/autologin",
    "description": "Autologin package for Laravel",
    "keywords": ["laravel", "autologin"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dwight Watson",
            "email": "dwightwatson@studious.com.au"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "illuminate/contracts": "6.*",
        "illuminate/support": "6.*",
        "nesbot/carbon": "2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.5.*"
    },
    "autoload": {
        "classmap": [
            "src/controllers"
        ],
        "psr-4": {
            "Watson\\Autologin\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Watson\\Autologin\\AutologinServiceProvider"
            ],
            "aliases": {
                "Autologin": "Watson\\Autologin\\Facades\\Autologin"
            }
        }
    },
    "minimum-stability": "stable"
}
